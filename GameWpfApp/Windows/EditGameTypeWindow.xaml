<Window x:Class="GameWpfApp.Windows.EditGameTypeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameWpfApp.Windows"
        mc:Ignorable="d"
        Title="EditGameTypeWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="20">
            <TextBlock Text="Game Type" TextAlignment="Center" FontSize="24" Margin="0,5,0,10"></TextBlock>
            <TextBox x:Name="TitleTb" Margin="10" />
            <Button x:Name="SaveBtn" Content="Save" Margin="10" Click="SaveBtn_Click"/>
        </StackPanel>
    </Grid>
</Window>
